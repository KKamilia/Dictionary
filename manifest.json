{
    "name": "Dictionary side panel",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Provides definitions in the side panel.",
    "background": {
        "service_worker": "service_worker.js"
    },
    "icons": {
        "128": "/images/metamask_128.png",
        "16": "/images/metamask_16.png"
    },
    "side_panel": {
        "default_path": "side_panel.html"
    },
    "permissions": [
        "sidePanel",
        "contextMenus"
    ]
}